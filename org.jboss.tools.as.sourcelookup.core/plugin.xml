<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension point="org.eclipse.debug.core.sourcePathComputers">
   		<sourcePathComputer
   			id="org.jboss.tools.as.sourcelookup.SourcePathComputer"
   			class="org.jboss.tools.as.sourcelookup.containers.JBossASSourcePathComputer">
   		</sourcePathComputer>
    </extension>
    
    <extension
         point="org.eclipse.debug.core.sourceContainerTypes">
      <sourceContainerType
            name="JBoss AS Source Container"
            class="org.jboss.tools.as.sourcelookup.containers.JBossSourceContainerType"
            id="org.jboss.tools.as.sourcelookup.containerType"
            description="JBoss AS Source Container">
      </sourceContainerType>
    </extension>
    
</plugin>
